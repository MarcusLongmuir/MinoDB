@background_color: rgba(240,240,240,1.0);

@border_color: gray;

@borderColor: rgba(0,0,0, 0.5);

.gradient{
	background: #1c2763; /* Old browsers */
	/* IE9 SVG, needs conditional override of 'filter' to 'none' */
	background: url(data:image/svg+xml;base64,);
	background: -moz-linear-gradient(45deg, #1c2763 0%, #5a3a77 45%, #9d4479 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left bottom, right top, color-stop(0%,#1c2763), color-stop(45%,#5a3a77), color-stop(100%,#9d4479)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(45deg, #1c2763 0%,#5a3a77 45%,#9d4479 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(45deg, #1c2763 0%,#5a3a77 45%,#9d4479 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(45deg, #1c2763 0%,#5a3a77 45%,#9d4479 100%); /* IE10+ */
	background: linear-gradient(45deg, #1c2763 0%,#5a3a77 45%,#9d4479 100%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#1c2763', endColorstr='#9d4479',GradientType=1 ); /* IE6-8 fallback on horizontal gradient */
}

.noselect{
	-webkit-tap-highlight-color: rgba(0,0,0,0);
  	-webkit-touch-callout: none;
	-webkit-user-select: none;
	-khtml-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	-o-user-select: none;
	user-select: none;
}

.bold{
	font-weight: bold;
}

.semibold{
	font-weight: bold;
}

