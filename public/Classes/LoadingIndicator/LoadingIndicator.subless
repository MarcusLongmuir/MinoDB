.loading_indicator{
	text-align: center;
	position: fixed;
	top: 50%;
	left: 50%;
	margin-left: -80px;
	margin-top: -15px;
	background-color: white;
	border: 1px solid #AAA;
	width: 160px;
	height: 30px;
	line-height: 30px;
	font-weight: bold;

	.loading_cancel_button{
		float: right;
		width: 60px;
		margin-top: -1px;
		margin-right: -1px;
		height: 32px;
	}

	.loader{
	    text-align: center;
	    vertical-align: middle;
	    width: 24px;
	    height: 24px;
	    color: @minoColor1;
	}

}